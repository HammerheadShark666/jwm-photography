@{
    ViewData["Title"] = "Import Photos";
}

<h5>@ViewData["Title"]</h5>
  
<div class="row">
    <form id="import-photos" method="post" enctype="multipart/form-data">   
        <div class="input-group">
            <input type="file" class="form-control" name="files" id="photos-import-upload" aria-label="Upload" multiple>
            <button class="btn btn-outline-secondary" type="submit" id="import-photos-btn">Import</button>
        </div>
    </form>
</div>

<div id="import-alerts" class="row">
</div>
 
<div id="photos-grid-container">
</div>

<input type="hidden" id="selected-photo-id" value="1" />
  
@section Scripts
{
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jsgrid/1.5.3/jsgrid.min.js"></script>   
    <script type="module" src="~/js/min/admin-photo-import.min.js" asp-append-version="true"></script>
}